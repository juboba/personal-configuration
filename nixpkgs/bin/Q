#!/usr/bin/env bash

SERVICE=$(echo -e "memrise\nramda\nrepl.it\nultimate guitar" | rofi -dmenu -p "URL")

case $SERVICE in
  "memrise") qutebrowser memrise.com ;;
  "ramda") qutebrowser ramdajs.com/repl ;;
  "repl.it") qutebrowser repl.it ;;
  "ultimate guitar") qutebrowser ultimate-guitar.com ;;
  "") exit 0 ;;
  *) qutebrowser "$SERVICE"
esac
